<%if flash[:message] %>
<%= flash[:message] %>
<%end%>



<%= form_for @employee do |f| %>
First name: <%= f.text_field :first_name %><br />
Last name : <%= f.text_field :last_name %><br />
Title:    <%= f.text_field :title %><br />
Office:   <%= f.text_field :office %><br />
alias:   <%= f.text_field :alias%><br />
Image:   <%= f.text_field :img_url %><br />
<br>Choose a dog: <%=f.collection_select :dog_id, Dog.all, :id, :name%><br>
<br><%=f.collection_check_boxes :dog_id, Dog.all, :id, :name%>
  <br><%= f.submit "Create a new Employee" %>
<% end %>